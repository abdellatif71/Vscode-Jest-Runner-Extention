{
  "name": "my-jest-runner",
  "displayName": "My Jest Runner",
  "description": "A simple VSCode extension to run Jest tests",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.runJestTest"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.runJestTest",
        "title": "Run Jest Test"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p .",
    "compile": "tsc -watch -p ."
  },
  "devDependencies": {
    "@types/node": "^14.0.0",
    "@types/vscode": "^1.50.0",
    "jest": "^29.7.0",
    "typescript": "^4.0.0"
  }
}
